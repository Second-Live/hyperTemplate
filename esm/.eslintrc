{
  "extends": "eslint:recommended",
  "globals": {
    "setTimeout": true,
    "clearTimeout": true,
    "MutationObserver": true,
    "Promise": true,
    "document": true
  },
  "parserOptions": {
    "sourceType": "module"
  },
  "rules": {
    "no-case-declarations": 0,
    "no-fallthrough": 0
  }
}